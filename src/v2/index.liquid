      <span>case studies</span>
    </a>
  </div>
  <div class="cover"></div>
</section>

<hr />
<div id="casestudy"></div>

{% for item in collections.works reversed %}
<section class="middle full home-{{ item.data.qr }}">
  <h2 class="hidden">{{ item.data.title }}</h2>
  <div class="padding">
    {% if item.data.iconTitle %}
      <h2 class="{{ item.data.qr }}title"><a href="{{ item.url }}" aria-label="{{ item.data.title }}">{{ item.data.iconTitle }}</a></h2>
    {% else %}
      <a href="{{ item.url }}" aria-label="{{ item.data.title }}">
        {% if item.data.icon %}
          {% assign icon = item.data.icon | split: ", " %}
          <svg width="{{ icon[1] }}" height="{{ icon[2] }}" viewBox="0 0 {{ icon[3] }}" class="logo {{ item.data.qr }}" aria-label="{{ item.data.iconLabel }}">
            <use href="/img/logo.svg#{{ icon[0] }}"></use>
          </svg>
        {% endif %}

        {% if item.data.iconVertical %}
          {% assign icon = item.data.iconVertical | split: ", " %}
          <svg width="{{ icon[1] }}" height="{{ icon[2] }}" viewBox="0 0 {{ icon[3] }}" class="logo {{ item.data.qr }} vertical" aria-label="{{ item.data.iconLabel }}">
            <use href="/img/logo.svg#{{ icon[0] }}"></use>
          </svg>
        {% endif %}

        {% if item.data.iconHorizontal %}
          {% assign icon = item.data.iconHorizontal | split: ", " %}
          <svg width="{{ icon[1] }}" height="{{ icon[2] }}" viewBox="0 0 {{ icon[3] }}" class="logo {{ item.data.qr }} horizontal" aria-label="{{ item.data.iconLabel }}">
            <use href="/img/logo.svg#{{ icon[0] }}"></use>
          </svg>
        {% endif %}
      </a>
    {% endif %}
  </div>
  <div class="arrow-small">
    <a href="{{ item.url }}" aria-label="{{ item.data.title }}" class="arrow">
      <span>{{ item.data.type }}</span>
    </a>
  </div>
  <div class="cover"></div>
</section>

<hr />
{% endfor %}

